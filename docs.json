{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Thesis",
  "colors": {
    "primary": "#16A34A",
    "light": "#FFFFFF",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["Overview", "Quickstart"]
          },
          {
            "group": "API Reference",
            "pages": [
              "api-reference/introduction",
              "api-reference/create",
              "api-reference/get",
              "api-reference/delete",
              "api-reference/webhook"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/markdown",
              "concepts/code",
              "concepts/images",
              "concepts/reusable-snippets"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/cursor",
              "integrations/windsurf",
              "integrations/claude"
            ]
          }
        ]
      },
      {
        "tab": "Examples",
        "groups": [
          {
            "group": "Live Demos",
            "pages": ["examples/live-demos/token-signal-telegram"]
          },
          {
            "group": "Tutorials",
            "pages": ["examples/tutorials/hello-world"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://status.thesis.io",
          "icon": "chart-line"
        },
        {
          "anchor": "Github",
          "href": "https://github.com/oraichain/thesis",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/f8u65hVUPv",
          "icon": "discord"
        },
        {
          "anchor": "Telegram",
          "href": "https://t.me/thesis_io",
          "icon": "paper-airplane"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://t.me/thesis_io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.thesis.io/settings"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/thesis_io",
      "github": "https://github.com/oraichain",
      "linkedin": "https://linkedin.com/company/oraichain"
    }
  }
}
